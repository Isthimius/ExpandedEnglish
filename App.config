<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ExpandedEnglish" type="ExpandedEnglish.Config.ExpandedEnglishConfig, ExpandedEnglish"/>
  </configSections>

  <ExpandedEnglish>
    <Conversions>
      <add From="too" To="three" />
      <add From="to" To="three" />
      <add From="won" To="two" />
    </Conversions>
  </ExpandedEnglish>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>